library(ggplot2)

data = read.csv('GenderPGap.csv')


#plotting
ggplot(data, aes(x = Education, y = BasePay))+ 
ggtitle("Plot") + theme(plot.title = element_text(hjust = 0.5)) +
xlab("x-axis") + ylab("y-axis")+geom_point(size = 5)

